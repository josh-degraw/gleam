type Person =
| Teacher of name: string * age: list<int> * title: string
| Student of name: string * age: list<int>
    member this.age = 
        match this with
        | Person.Teacher (_, age, _) -> age
        | Person.Student (_, age) -> age
    member this.name = 
        match this with
        | Person.Teacher (name, _, _) -> name
        | Person.Student (name, _) -> name

let get_name (person: Person): string = begin
    person.name
end
let get_age (person: Person): list<int> = begin
    person.age
end

type Person = { username: string; name: string; age: int }
let go () = begin
    let given_name = "jack"
    let raiden = { username = "raiden"; name = "jack"; age = 31 }
    match given_name with
    | name when name = raiden.name ->
        "It's jack"
    | _ ->
        "It's not jack"
end
